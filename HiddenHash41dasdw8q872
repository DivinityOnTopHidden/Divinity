local ToggleCdcGlass = false
local ToggleCdcGlass = MapSection3:AddToggle({
    Name = "Toggle CDC Tower Glass",
    Callback = function(ToggleCdcGlass)
        togglecdcglass = ToggleCdcGlass
    if togglecdcglass then
        for _,v in pairs(workspace:GetDescendants()) do
            if v.Name == "CDCInvisibleParts" then
                v.Parent = game.Lighting
            end
        end
        for _,v in pairs(workspace:GetDescendants()) do
            if v:IsA("Part") then
                if v.CFrame == CFrame.new(21.7980118, 64.3996048, 473.020752, -1, 0, -0, 0, 0, -1, 0, -1, -0) then
                    v.Parent = game.Lighting
                end
            end
        end
        for _,v in pairs(workspace:GetDescendants()) do
            if v:IsA("Part") then
                if v.CFrame == CFrame.new(10.7295303, 57.4499741, 423.218628) then
                    v.Parent = game.Lighting
                end
            end
        end
        for _,v in pairs(workspace:GetDescendants()) do
            if v:IsA("Part") then
                if v.CFrame == CFrame.new(31.7795181, 57.4499741, 423.218628) then
                    v.Parent = game.Lighting
                end
            end
        end
    elseif not togglecdcglass then
        for _,v in pairs(game.Lighting:GetDescendants()) do
            if v.Name == "CDCInvisibleParts" then
                v.Parent = game.Workspace
            end
        end
        for _,v in pairs(game.Lighting:GetDescendants()) do
            if v:IsA("Part") then
                if v.CFrame == CFrame.new(21.7980118, 64.3996048, 473.020752, -1, 0, -0, 0, 0, -1, 0, -1, -0) then
                    v.Parent = game.Workspace
                end
            end
        end
        for _,v in pairs(game.Lighting:GetDescendants()) do
            if v:IsA("Part") then
                if v.CFrame == CFrame.new(10.7295303, 57.4499741, 423.218628) then
                    v.Parent = game.Workspace
                end
            end
        end
        for _,v in pairs(game.Lighting:GetDescendants()) do
            if v:IsA("Part") then
                if v.CFrame == CFrame.new(31.7795181, 57.4499741, 423.218628) then
                    v.Parent = game.Workspace
                end
            end
        end
    end
end})
local ToggleDoors = false
local ToggleDoors = MapSection3:AddToggle({
    Name = "Toggle Doors",
    Callback = function(ToggleDoors)
        toggledoors = ToggleDoors
    if toggledoors then
        for _,v in pairs(workspace:GetDescendants()) do
            if v:IsA("Model") then
                if v.Name == "Gate" or v.Name == "Transparent Door" or v.Name == "Linked Door" or v.Name == "Double Gate" or v.Name == "Heavy Door" then
                    v.Parent = game.Lighting
                end
            end
        end
    elseif not toggledoors then
        for _,v in pairs(game.Lighting:GetDescendants()) do
            if v:IsA("Model") then
                if v.Name == "Gate" or v.Name == "Transparent Door" or v.Name == "Linked Door" or v.Name == "Double Gate" or v.Name == "Heavy Door" then
                    v.Parent = game.Workspace
                end
            end
        end
    end
end})
local ToggleNightSky = false
local ToggleNightSky = MapSection3:AddToggle({
    Name = "Toggle Night Sky",
    Callback = function(ToggleNightSky)
        togglenightsky = ToggleNightSky
    if togglenightsky then
        game.Lighting.Sky.Parent = game
    elseif not togglenightsky then
        game.Sky.Parent = game.Lighting
    end
end})
