        if workspace:FindFirstChild("Chaos Insurgency") then
            workspace["Chaos Insurgency"]:Destroy()
        end
        if workspace:FindFirstChild("Leaderboards") then
            workspace.Leaderboards:Destroy()
        end
        if workspace:FindFirstChild("Lights") then
            workspace.Lights:Destroy()
        end
        if workspace:FindFirstChild("S3EasterEgg") then
            workspace.S3EasterEgg:Destroy()
        end
        if workspace:FindFirstChild("Sector 1") then
            workspace["Sector 1"]:Destroy()
        end
        if workspace:FindFirstChild("Sector 2") then
            workspace["Sector 2"]:Destroy()
        end
        if workspace:FindFirstChild("Medical Sector") then
            workspace["Medical Sector"]:Destroy()
        end
        if workspace:FindFirstChild("PlatformPart") then
            workspace.PlatformPart:Destroy()
        end
        local PlatformPart = Instance.new("Part")
        PlatformPart.Name = "PlatformPart"
        PlatformPart.Anchored = true
        PlatformPart.Size = Vector3.new(100000,1,100000)
        PlatformPart.Transparency = 0.6
        PlatformPart.Material = "Glass"
        PlatformPart.Color = Color3.fromRGB(255,255,255)
        PlatformPart.Parent = workspace
