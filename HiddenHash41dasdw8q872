-- Functions:
function promptaura()
    for i, v in pairs(game:GetService("Workspace"):GetDescendants()) do
        if v:IsA("Part") or v:IsA("MeshPart") then
            if game:GetService("Players").LocalPlayer:DistanceFromCharacter(v.Position) <= 15 then
                if v:FindFirstChild("ProximityPrompt") then
                    fireproximityprompt(v.ProximityPrompt,1,true)
                elseif v:FindFirstChild("InteractionPrompt") then
                    fireproximityprompt(v.InteractionPrompt,1,true)
                end
            end
        end
    end
end
function shoppart()
    if workspace:FindFirstChild("ShopPlatformPart") then
        workspace.ShopPlatformPart:Destroy()
    end
    local ShopPlatformPart = Instance.new("Part")
    ShopPlatformPart.Name = "ShopPlatformPart"
    ShopPlatformPart.Anchored = true
    ShopPlatformPart.Size = Vector3.new(3,1,3)
    ShopPlatformPart.Transparency = 1
    ShopPlatformPart.Material = "Glass"
    ShopPlatformPart.Color = Color3.fromRGB(255,255,255)
    ShopPlatformPart.Parent = workspace
    ShopPlatformPart.CFrame = CFrame.new(125.4761734008789, 29.974639892578125, 340.6395568847656)
    task.wait(.3)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace.ShopPlatformPart.CFrame * CFrame.new(0,1,0)
end
-- End Of Functions


