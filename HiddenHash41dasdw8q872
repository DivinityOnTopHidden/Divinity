CharacterSection4:AddButton({
    Name = "Reset Quests",
    Callback = function()
        if game.Players.LocalPlayer.Team.Name == "Class - D" then
            saved = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
            local args = {
                [1] = {
                    [1] = "Security Department",
                    [2] = 0,
                    [3] = false
