local RainbowOutfit2 = false
local RainbowOutfit2 = CharacterSection3:AddToggle({
    Name = "Rainbow Outfit",
    Callback = function(RainbowOutfit2)
        rainbowoutfit2 = RainbowOutfit2
end})
local RainbowOutfitLoop = game:GetService("RunService").RenderStepped:Connect(function()
    if rainbowoutfit2 then
        local hue = tick() % 3 / 3
        game.Players.LocalPlayer.Character.Clothing.Color3 = Color3.fromHSV(hue, 1, 1);
        game.Players.LocalPlayer.Character:GetChildren()[24].Color3 = Color3.fromHSV(hue, 1, 1);
        game.Players.LocalPlayer.Character:GetChildren()[23].Color3 = Color3.fromHSV(hue, 1, 1);
    end
    if not rainbowoutfit2 then
        game.Players.LocalPlayer.Character.Clothing.Color3 = Color3.fromRGB(255,255,255)
        game.Players.LocalPlayer.Character:GetChildren()[24].Color3 = Color3.fromRGB(255,255,255)
        game.Players.LocalPlayer.Character:GetChildren()[23].Color3 = Color3.fromRGB(255,255,255)
    end
end)
local RainbowSkin = false
local RainbowSkin = CharacterSection3:AddToggle({
    Name = "Rainbow Skin",
    Callback = function(RainbowSkin)
        rainbowskin = RainbowSkin
end})
local RainbowSkinLoop = game:GetService("RunService").RenderStepped:Connect(function()
    if rainbowskin then
        local hue = tick() % 3 / 3
        game.Players.LocalPlayer.Character["Body Colors"].HeadColor3 = Color3.fromHSV(hue, 1, 1);
        game.Players.LocalPlayer.Character["Body Colors"].LeftArmColor3 = Color3.fromHSV(hue, 1, 1);
        game.Players.LocalPlayer.Character["Body Colors"].RightArmColor3 = Color3.fromHSV(hue, 1, 1);
        game.Players.LocalPlayer.Character["Body Colors"].LeftLegColor3 = Color3.fromHSV(hue, 1, 1);
        game.Players.LocalPlayer.Character["Body Colors"].RightLegColor3 = Color3.fromHSV(hue, 1, 1);
        game.Players.LocalPlayer.Character["Body Colors"].TorsoColor3 = Color3.fromHSV(hue, 1, 1);
    end
    if not rainbowskin then
        game.Players.LocalPlayer.Character["Body Colors"].HeadColor3 = Color3.fromRGB(211, 161, 123)
        game.Players.LocalPlayer.Character["Body Colors"].LeftArmColor3 = Color3.fromRGB(211, 161, 123)
        game.Players.LocalPlayer.Character["Body Colors"].RightArmColor3 = Color3.fromRGB(211, 161, 123)
        game.Players.LocalPlayer.Character["Body Colors"].LeftLegColor3 = Color3.fromRGB(211, 161, 123)
        game.Players.LocalPlayer.Character["Body Colors"].RightLegColor3 = Color3.fromRGB(211, 161, 123)
        game.Players.LocalPlayer.Character["Body Colors"].TorsoColor3 = Color3.fromRGB(211, 161, 123)
    end
end)
local RainbowGuns = false
local RainbowGuns = CharacterSection3:AddToggle({
    Name = "Rainbow Guns",
    Callback = function(RainbowGuns)
        rainbowguns = RainbowGuns
end})
local RainbowGunsLoop = game:GetService("RunService").RenderStepped:Connect(function()
    if rainbowguns then
        local hue = tick() % 3 / 3
        for _,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
            if v:IsA("Tool") then
                for _,x in pairs(v:GetDescendants()) do
                    if x:IsA("MeshPart") or x:IsA("Part") then
                        x.Color = Color3.fromHSV(hue, 1, 1);
                    end
                end
            end
        end
    end
    if not rainbowguns then
        for _,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
            if v:IsA("Tool") then
                for _,x in pairs(v:GetDescendants()) do
                    if x:IsA("MeshPart") or x:IsA("Part") then
                        x.Color = Color3.fromHSV(92, 90, 93);
                    end
                end
            end
        end
    end
end)
