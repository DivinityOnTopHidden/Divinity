-- Character Tab:
local BetterNVGs = false
local BetterNVGs = CharacterSection1:AddToggle({
    Name = "Better NVG",
    Callback = function(BetterNVGs)
        betternvg = BetterNVGs
end})
local BetterNVGLoop = game:GetService("RunService").RenderStepped:Connect(function()
    if betternvg then
        if game:GetService("Players").LocalPlayer.PlayerGui.WeaponUI.NVG.Visible == true then
            game:GetService("Players").LocalPlayer.PlayerGui.WeaponUI.NVG.Visible = false
        end
        game:GetService("Players").LocalPlayer.PlayerGui.WeaponUI.NVG.Visible = false
        if game:GetService("Lighting").NVG.Enabled == true then
            game:GetService("Lighting").NVGBlur.Enabled = false
        end
    end
    if not betternvg then
        if game:GetService("Players").LocalPlayer.PlayerGui.WeaponUI.NVG.Visible == false then
            game:GetService("Players").LocalPlayer.PlayerGui.WeaponUI.NVG.Visible = true
        end
    end
end)

local ToggleChat = false
local ToggleChat = CharacterSection1:AddToggle({
    Name = "Toggle Chat",
    Callback = function(ToggleChat)
        togglechat = ToggleChat
    if togglechat then
        game.Players.LocalPlayer.PlayerGui.Chat.Frame.ChatChannelParentFrame.Visible = true
        game.Players.LocalPlayer.PlayerGui.Utility.Credits.Visible = false
        game.Players.LocalPlayer.PlayerGui.Utility.Version.Visible = false
    elseif not togglechat then
        game.Players.LocalPlayer.PlayerGui.Chat.Frame.ChatChannelParentFrame.Visible = false
        game.Players.LocalPlayer.PlayerGui.Utility.Credits.Visible = true
        game.Players.LocalPlayer.PlayerGui.Utility.Version.Visible = true
    end
end})

